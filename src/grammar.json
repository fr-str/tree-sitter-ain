      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "(?i)[0-9a-z/_-]+"
    },
    "bool": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 2,
        "content": {
          "type": "PATTERN",
          "value": "(?i)true|false"
        }
      }
