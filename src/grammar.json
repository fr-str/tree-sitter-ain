{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ain",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "section"
          }
        }
      ]
    },
    "section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "host"
        },
        {
          "type": "SYMBOL",
          "name": "query"
        },
        {
          "type": "SYMBOL",
          "name": "headers"
        },
        {
          "type": "SYMBOL",
          "name": "method"
        },
        {
          "type": "SYMBOL",
          "name": "body"
        }
      ]
    },
    "host": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "(?i)host"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "value"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "query": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "(?i)query"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "query_key_val"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "query_key": {
      "type": "SYMBOL",
      "name": "string"
    },
    "query_val": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bool"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "query_key_val": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "query_key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "query_val"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "headers": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "(?i)headers"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "header_key_val"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "header_key": {
      "type": "SYMBOL",
      "name": "string"
    },
    "header_val": {
      "type": "SYMBOL",
      "name": "value"
    },
    "header_key_val": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_key"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": " "
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "header_val"
        }
      ]
    },
    "method": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "(?i)method"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "value"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "body": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "(?i)body"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "json_body"
                },
                {
                  "type": "SYMBOL",
                  "name": "urlencoded_body"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "json_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "json_object"
          }
        },
        {
          "type": "SYMBOL",
          "name": "json_array"
        }
      ]
    },
    "json_object": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "json_pair"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "json_pair"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "json_array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "json_value"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "json_value"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "json_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "json_string"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "json_value"
        }
      ]
    },
    "json_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "json_object"
        },
        {
          "type": "SYMBOL",
          "name": "json_array"
        },
        {
          "type": "SYMBOL",
          "name": "json_string"
        },
        {
          "type": "SYMBOL",
          "name": "json_number"
        },
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        },
        {
          "type": "STRING",
          "value": "null"
        }
      ]
    },
    "json_string": {
      "type": "PATTERN",
      "value": "\"(?:[^\"\\\\]|\\\\.)*\""
    },
    "json_number": {
      "type": "PATTERN",
      "value": "-?\\d+(?:\\.\\d+)?(?:[eE][+-]?\\d+)?"
    },
    "plain_body": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[^\\n]+"
      }
    },
    "urlencoded_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "urlencoded_key_val"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "urlencoded_amper"
              },
              {
                "type": "SYMBOL",
                "name": "urlencoded_key_val"
              }
            ]
          }
        }
      ]
    },
    "urlencoded_key": {
      "type": "PATTERN",
      "value": "[^%][a-z0-9A-Z-_]+"
    },
    "urlencoded_val": {
      "type": "PATTERN",
      "value": "[^%\\n]+|%[^26][^%\\n]*|%2[^6][^%\\n]*|%[^2][^%\\n]*"
    },
    "urlencoded_equals": {
      "type": "STRING",
      "value": "%3D"
    },
    "urlencoded_amper": {
      "type": "STRING",
      "value": "%26"
    },
    "urlencoded_key_val": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "urlencoded_key"
        },
        {
          "type": "SYMBOL",
          "name": "urlencoded_equals"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "urlencoded_val"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9.]+"
    },
    "string": {
      "type": "PATTERN",
      "value": "(?i)[0-9a-z/_-]+"
    },
    "bool": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 2,
        "content": {
          "type": "PATTERN",
          "value": "(?i)true|false"
        }
      }
    },
    "_": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "#.*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "query_key_val"
    ],
    [
      "body"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}