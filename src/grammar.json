      "type": "PATTERN",
      "value": "\"(?:[^\"\\\\]|\\\\.)*\""
    },
    "json_number": {
      "type": "PATTERN",
      "value": "-?\\d+(?:\\.\\d+)?(?:[eE][+-]?\\d+)?"
